#include<bits/stdc++.h>
#include<iostream>
using namespace std;

int main()
{
    int n;
    cin>>n;
    vector<int> p(n);
    
    for(int i=0;i<n;i++)
    cin>>p[i];
    
    int dp[n+1];
    dp[0]=0;
    for(int i=1;i<=n;i++)
    dp[i]=p[i-1];
    
/*p[0] stores the price of length= 1. so we are initialising the dp array with this price 
array.*/
    for(int i=1;i<=n;i++)
    {
        for(int j=0;j<i;j++)
        dp[i] = max(dp[i],dp[j]+dp[i-j]);
    }
    cout<<dp[n]<<endl;
    
}
